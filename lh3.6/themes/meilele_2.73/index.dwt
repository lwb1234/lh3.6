function formatTime(i) {
	if (!i) {
		return "刚刚";
	}
	var b = parseInt((new Date()).getTime() / 1000);
	var f = b - i;
	if (f < 0) {
		f = 0;
	}
	var e = f % 60;
	var a = parseInt(f % 3600 / 60);
	var c = parseInt(f % (3600 * 24) / 3600);
	var g = parseInt(f / (3600 * 24));
	if (g) {
		return g + "天前";
	} else {
		if (c) {
			return c + "小时前";
		} else {
			if (a) {
				return a + "分钟前";
			} else {
				if (e) {
